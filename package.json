    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^22.4.3",
    "prettier": "^1.12.1",
    "semantic-release": "^15.2.0",
    "travis-deploy-once": "^5.0.0"
